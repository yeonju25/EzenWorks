package com.ezen.reflect.controller;

public class UserController {
	
	public void join() {
		System.out.println("join()함수 호출");
	}
	
	public void login() {
		System.out.println("login()함수 호출");
	}
	
	public void user() {
		System.out.println("user()함수 호출");
	}

	public void hello() {
		System.out.println("hello()함수 호출");
	}
	
	
	
}
